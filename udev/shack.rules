SUBSYSTEM=="tty", ATTRS{idVendor}=="16d0", ATTRS{idProduct}=="087e", SYMLINK+="ttyUSBdigikeyer", ENV{SYSTEMD_USER_WANTS}+="cwdaemon.service"

